// Write a C Program to identify if a number is a palindrome or not.

#include <stdio.h>


int main() 
{
    int n,revN = 0,r,orgN;
        
        scanf("%d",&n);
    
    orgN = n;
    
    while (n!=0)
    {
        r=n % 10;
        revN = revN*10+r;
        n = n/10;
    }
    
    if(orgN==revN)
       printf("Yes");
else
    printf("No");
return 0;
}

   
