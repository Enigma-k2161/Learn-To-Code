//Write a C Program to print "Hello World!" and "Welcome to Learn To Code!" in two separate lines

#include <stdio.h>
int main() 
{
    printf("Hello World!\nWelcome to Learn To Code!");
    return 0;
}

//Write a C Program to perform Read and Print operations using the four basic data types in C. (int, float, char & double)

#include <stdio.h>
int main() {
    char x;
    int y;
    float z;
    double a;
    
    scanf("%c", &x);
    scanf("%d", &y);
    scanf("%f", &z);
    scanf("%lf", &a);
    
    printf("%c\n",x);
    printf("%d\n",y);
    printf("%f\n",z);
    printf("%lf\n",a);
}

    
