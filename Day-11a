//Write a C program to read n values from the user and store it in the array. Reprint the values stored in the array and also print the value of second element in the array.

#include <stdio.h>    
     
int main()    
{    
    int arr1[100], n;    
        
    scanf("%d\n",&n);
    
    for (int i = 0; i < n; i++) 
    {     
        scanf("%d\n",&arr1[i]);    
    }   
    
    int arr2[n];    
        
    for (int i = 0; i < n; i++) 
    {     
        arr2[i] = arr1[i];     
    }          
        
    printf("Entered array: ");    
    for (int i = 0; i < n; i++) {     
        printf("%d ", arr2[i]);    
    }    
    printf("\n%d", arr2[1]);
    return 0;    
}
